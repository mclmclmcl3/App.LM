<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Primary" Color="#0275d8"/>
    <SolidColorBrush x:Key="Secondary" Color="#555555"/>
    <SolidColorBrush x:Key="Success" Color="#5cb85c"/>
    <SolidColorBrush x:Key="Info" Color="#5bc0de"/>
    <SolidColorBrush x:Key="Warning" Color="#f0ad4e"/>
    <SolidColorBrush x:Key="Danger" Color="#d9534f"/>
    <SolidColorBrush x:Key="Negro" Color="#292b2c"/>
    <SolidColorBrush x:Key="Blanco" Color="#f7f7f7"/>
    <SolidColorBrush x:Key="Claro" Color="#FFD0D0D0"/>
    <SolidColorBrush x:Key="Oscuro" Color="#FF3E3E3E"/>

    <SolidColorBrush x:Key="DarkGrey" Color="#F7323232"/>
    <SolidColorBrush x:Key="Gray" Color="Gray"/>
    <SolidColorBrush x:Key="LightGray" Color="#FFD0D0D0"/>

</ResourceDictionary>